{
  "name": "iExtension",
  "manifest_version": 2,
  "version": "1.0",
  "description": "using iExtension",
  "browser_action": {
    "default_title": "iExtension", 
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["script.js"]
    }
  ]
}